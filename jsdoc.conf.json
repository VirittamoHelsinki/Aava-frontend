{
    "tags": {
      "allowUnknownTags": true
    },
    "source": {
        "include": ["./src"],
        "includePattern": "\\.(vue|js|jsx)$"
    },
    "plugins": [
        "plugins/markdown", 
        "better-docs/component",
        "better-docs/category"
    ],
    "templates": {
        "better-docs": {
            "name": "Aava documentation for react components"
        }
    },
    "recurseDepth": 10,
    "opts": {
      "destination": "docs",
      "recurse": true,
      "readme": "./README.md",
      "template": "./node_modules/better-docs"
    }
  }